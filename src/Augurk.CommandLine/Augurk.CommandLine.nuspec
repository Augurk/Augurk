<?xml version="1.0"?>
<package >
	<metadata>
		<id>Augurk.CommandLine</id>
		<version>$version$</version>
		<title>Augurk.CommandLine</title>
		<authors>Jonathan Mezach, Mark Taling</authors>
		<owners>Mark Taling</owners>
		<licenseUrl>https://marktaling.github.io/Augurk/License.md</licenseUrl>
		<projectUrl>https://github.com/marktaling/Augurk</projectUrl>
		<iconUrl>https://marktaling.github.io/Augurk/img/icon128.png</iconUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>Command line tools for publishing feature files to Augurk.</description>
		<releaseNotes>
      Fixes an issue that occurs when a version is not specified while publishing feature files to Augurk.
      A version number of 0.0.0 is now implied if no version number is specified.
    </releaseNotes>
		<copyright>Copyright 2015</copyright>
		<tags>augurk gherkin specflow commandline</tags>
	</metadata>
	<files>
		<file src="bin\$configuration$\augurk.exe" target="tools" />
		<file src="bin\$configuration$\augurk.exe.config" target="tools" />
		<file src="bin\$configuration$\*.dll" target="tools" />
	</files>
</package>