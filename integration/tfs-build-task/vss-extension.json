{
  "manifestVersion": 1,
  "id": "augurk-build-task",
  "name": "Augurk Build Integration",
  "version": "0.5.0",
  "publisher": "Augurk",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Build Integration for publishing feature files to Augurk",
  "categories": [
    "Build and release"
  ],
  "icons": {
    "default": "PublishFeaturesToAugurk/icon.png"
  },
  "files": [
    {
      "path": "PublishFeaturesToAugurk"
    }
  ],
  "contributions": [
    {
      "id": "publishfeaturestoaugurk",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "PublishFeaturesToAugurk"
      }
    }
  ]
}